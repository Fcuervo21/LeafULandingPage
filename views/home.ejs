                        </div>
                    </div>
                </div>

                <div class="right-container">
                    <h3 class="h3-sec6 move-h3">Ask us something.</h3>
                    <form action="/" method="post" novalidate>
                        <div class="right-container-size">
                            <div class="title-container">
                                <label>Name</label>
                            </div>
                            <input class="sec6-inputs" id="name" type="text" name="name" autocomplete="off"
                                value="<% if(typeof valores != 'undefined') { %><%= valores.name %><% } %>" />
                            <div class="title-container">
                                <label for="">Email</label>
                            </div>
                            <input class="sec6-inputs" id="email" type="email" name="email" autocomplete="off"
                                value="<% if(typeof valores != 'undefined') { %><%= valores.email %><% } %>" />
                            <div class="title-container">
                                <label>Message</label>
                            </div>
                            <textarea id="message" type="text" name="message" autocomplete="off" rows="9"
                                value="<% if(typeof valores != 'undefined') { %><%= valores.message %><% } %>"></textarea>
                            <button class="btn-primary sec6-button" type="submit">
                                Send
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </section>

